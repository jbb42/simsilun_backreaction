set terminal pdfcairo enhanced color dashed dashlength 2.0 font "Helvetica,20"
set xrange[-0.001:0.001]
set yrange[0.1:10]
set log y
set xtics -0.001,0.0005,0.003
set xlabel "{/Symbol d}_i"
set ylabel "{/Symbol d}_0 + 1"
set output "density.pdf"
plot "density" u 1:2 notitle w l lw 2 lc rgb "#358BE6", \
     "density" u 1:3 notitle w l lw 2 lc rgb "#FF2E2E"
